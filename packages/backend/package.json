{
  "name": "@restaurant-reservation/backend",
  "version": "1.0.0",
  "description": "Backend service for restaurant reservation system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
